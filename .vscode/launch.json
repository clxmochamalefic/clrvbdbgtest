{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NETFW launch (console)",
            "type": "clr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ConsoleApp1/bin/x64/Debug/ConsoleApp1.exe",
			"args": [
				"${workspaceRoot}/ConsoleApp1/ConsoleApp1.vbproj",
				"/p:Configuration=Debug;GenerateFullPaths=true",
				"/t:build"
            ],
            "cwd": "${workspaceFolder}/ConsoleApp1/bin/x64/Debug/",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NETFW Attach",
            "type": "clr",
            "request": "attach"
        }
    ]
}